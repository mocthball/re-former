<h1> Users Page </h1>

 <%= form_with model: User.new do |form| %>
    <%= label_tag("Username:") %>
    <%= form.text_field :username, placeholder: "Enter Username" %>
    <%= form.text_field :email, placeholder: "email@address.com" %>
    <%= form.password_field :password %>
    <%= submit_tag("Submit") %>
  <% end %>

<%= link_to "Edit", "/edit" %>

